{
  "name": "@daracl/loading",
  "version": "0.0.6",
  "sideEffects": false,
  "description": "Daracl Loading Message",
  "author": {
    "name": "darainfo",
    "email": "darainfosys@gmail.com",
    "url": "https://www.moaview.com"
  },
  "license": "MIT",
  "keywords": [
    "@daracl/loading",
    "daracl-loading",
    "loading",
    "darainfo",
    "message",
    "notification",
    "javascript"
  ],
  "main": "src/index.ts",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "homepage": "https://github.com/daracl/ui-loading",
  "bugs": {
    "url": "https://github.com/daracl/ui-loading/issues",
    "email": "darainfosys@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daracl/ui-loading.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./umd": {
      "import": "./dist/daracl.loading.umd.js"
    },
    "./dist/": "./dist/"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime-corejs3": "^7.25.7",
    "esbuild": "^0.18.11",
    "typescript": "^5.1.6"
  },
  "prettier": {
    "printWidth": 256,
    "useTabs": false,
    "tabWidth": 2,
    "bracketSpacing": true,
    "semi": true,
    "singleQuote": false
  },
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/parser": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.25.7",
    "@babel/plugin-transform-block-scoping": "^7.25.7",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/plugin-transform-strict-mode": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/runtime": "^7.25.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-loader": "^8.3.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "cpx": "^1.5.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.8",
    "sass": "^1.80.2",
    "sass-loader": "^16.0.2",
    "style-loader": "^3.3.3",
    "ts-node": "^10.9.1",
    "vite": "^5.4.0",
    "vite-plugin-banner": "^0.7.1"
  },
  "scripts": {
    "prepack": "yarn build",
    "build:js": "node build.js",
    "dts:gen": "npx -p typescript tsc src/index.js --declaration --allowJs --emitDeclarationOnly --outDir dist",
    "test": "jest",
    "dev": "vite",
    "build": "vite build --mode production && vite build --mode development && yarn copy-dts && yarn build:tsc && yarn copy-unmin && yarn clean:unmin && yarn copy-html",
    "build:tsc": "tsc --emitDeclarationOnly",
    "clean:unmin": "rimraf dist/unmin",
    "copy-html": "cpx \"public/index.html\" dist",
    "copy-unmin": "cpx \"dist/unmin/**.*\" dist",
    "copy-dts": "cpx \"src/**/*.d.ts\" dist"
  },
  "browserslist": {
    "production": [
      "ie 11",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
